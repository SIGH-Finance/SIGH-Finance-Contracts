		"devdoc": {
			"author": "_astromartian, built upon the Aave protocol's AddressesProviderContract(v1)*",
			"kind": "dev",
			"methods": {
				"getFeeProvider()": {
					"details": "returns the address of the FeeProvider proxy",
					"returns": {
						"_0": "the address of the Fee provider proxy*"
					}
				},
				"getLendingPool()": {
					"details": "returns the address of the LendingPool proxy",
					"returns": {
						"_0": "the lending pool proxy address*"
					}
				},
				"getLendingPoolConfigurator()": {
					"details": "returns the address of the LendingPoolConfigurator proxy",
					"returns": {
						"_0": "the lending pool configurator proxy address*"
					}
				},
				"getLendingPoolLiquidationManager()": {
					"details": "returns the address of the LendingPoolLiquidationManager. Since the manager is used through delegateCall within the LendingPool contract, the proxy contract pattern does not work properly hence the addresses are changed directly.",
					"returns": {
						"_0": "the address of the Lending pool liquidation manager*"
					}
				},
				"getPriceOracle()": {
					"details": "the functions below are storing specific addresses that are outside the context of the protocol hence the upgradable proxy pattern is not used*"
				},
				"getSIGHFinanceConfigurator()": {
					"details": "returns the address of the SIGHFinanceConfigurator proxy",
					"returns": {
						"_0": "the SIGH Finance configurator proxy address*"
					}
				},
				"getSIGHSpeedController()": {
					"details": "returns the address of the SIGH_SPEED_CONTROLLER proxy",
					"returns": {
						"_0": "the SIGH Speed Controller address*"
					}
				},
				"setFeeProviderImpl(address)": {
					"details": "updates the implementation of the FeeProvider proxy",
					"params": {
						"_feeProvider": "the new lending pool fee provider implementation*"
					}
				},
				"setLendingPoolConfiguratorImpl(address)": {
					"details": "updates the implementation of the lending pool configurator",
					"params": {
						"_configurator": "the new lending pool configurator implementation*"
					}
				},
				"setLendingPoolImpl(address)": {
					"details": "updates the implementation of the lending pool",
					"params": {
						"_pool": "the new lending pool implementation*"
					}
				},
				"setLendingPoolLiquidationManager(address)": {
					"details": "updates the address of the Lending pool liquidation manager",
					"params": {
						"_manager": "the new lending pool liquidation manager address*"
					}
				},
				"setSIGHFinanceConfiguratorImpl(address)": {
					"details": "updates the implementation of the lending pool configurator",
					"params": {
						"_configurator": "the new lending pool configurator implementation*"
					}
				},
				"setSIGHSpeedController(address)": {
					"details": "sets the address of the SIGH Speed Controller",
					"params": {
						"_SIGHSpeedController": "the SIGH Speed Controller implementation*"
					}
				},
				"setSIGHStaking(address)": {
					"details": "updates the address of the SIGH Distribution Handler Contract (Manages the SIGH Speeds)",
					"params": {
						"_SIGHStaking": "the new lending pool liquidation manager address*"
					}
				},
				"setSIGHTreasuryImpl(address)": {
					"details": "updates the address of the SIGH Treasury Contract",
					"params": {
						"_SIGHTreasury": "the new SIGH Treasury Contract address*"
					}
				},
				"setSIGHVolatilityHarvesterImpl(address)": {
					"details": "updates the address of the SIGH Distribution Handler Contract (Manages the SIGH Speeds)",
					"params": {
						"_SIGHVolatilityHarvester": "the new SIGH Distribution Handler (Impl) Address*"
					}
				}
			},
