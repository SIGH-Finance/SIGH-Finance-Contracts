					"params": {
						"_boosterId": "The boosterID of the Booster used to get a discount on the Fee*",
						"amount": "The amount borrowed out",
						"instrumentAddress": "The address of the underlying asset being borrowed",
						"platformFee": "Platform Fee charged",
						"reserveFee": "Reserve Fee charged",
						"user": "The address that will be getting the debt"
					}
				},
